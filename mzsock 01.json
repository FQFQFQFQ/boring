[{
        "name": "mzsock",
        "key":"mzsock",
        "url": "http://mzsock.com",
        "tags": [ 
            {"name": "棉袜", "url": "http://mzsock.com/mv/"},
			{"name": "船袜", "url": "http://mzsock.com/cy/"},
			{"name": "丝袜", "url": "http://mzsock.com/sw/"},
			{"name": "裸足", "url": "http://mzsock.com/lz/"},
			{"name": "帆布鞋", "url": "http://mzsock.com/fbx/"},
			{"name": "运动鞋", "url": "http://mzsock.com/ydx/"},
			{"name": "人字拖", "url": "http://mzsock.com/rzt/"},
			{"name": "美脚自拍", "url": "http://mzsock.com/cwzp/"},
        ], 
        "pages": { 
            "element": "body > div.recent-article cl > ul > li",
            "name": { 
                "element": "p.p_title > p", 
                "attr": "@text"
            },
            "url": {
                "element": "a",
                "attr": "href",
                "needMerge": true
            },
            "nextPageUrlMode": {
                "mode": "add", 
                "addValue": "/index_{page}.html"
            }, 
            "lastPage": {
                "element": ".page a", 
                "minPageLength": 2, 
                "attr": "@text", 
                "descIndex": 2, 
                "isNeedSplit": false, 
                "splits": []
            }
        }, 
        "imgs": {
            "element": ".photo-pic img", 
            "urlNeedMerge": true,
            "singlePage":true,
            "nextPageUrlMode": {
                "incrementMode": "page",
                "mode": "add", 
                "addValue": ""
            }, 
            "lastPage": {
                "element": "#pages > a"
            }
        }, 
        "supportReptileVersion": 2
    },
