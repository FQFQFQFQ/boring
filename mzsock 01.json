        "pages": { 
            "element": "body > div.recent-article cl > ul > li",
            "name": { 
                "element": "p.p_title > p", 
                "attr": "@text"
 },
            "url": {
                "element": "a",
                "attr": "href",
                "needMerge": true
            },
            "nextPageUrlMode": {
                "mode": "add",  
                "addValue": ""
            }, 
            "lastPage": {
                "element": "#pageNum > a", 
                "minPageLength": 2, 
                "attr": "@text", 
                "descIndex": 2, 
                "isNeedSplit": false, 
                "splits": []
            }
        }, 
        "imgs": {
            "element": ".gallery img", 
            "attr":"data-original",
            "singlePage": true,
            "imgUrlMode":{
                "mode":"replace",
                "addValue": "",
                "replaceSearchValue": "//", 
                "replaceValue": "http://"
            },
            "nextPageUrlMode": { 
                "incrementMode": "page"
            }
        }, 
        "supportReptileVersion": 2
    },
